{
  "name": "minecraft-server-cli",
  "version": "0.0.0",
  "description": "A CLI application to manage and launch a minecraft server.",
  "author": "TheAce0296",
  "license": "MIT",
  "homepage": "https://github.com/TheAce0296/minecraft-server-cli",
  "repository": "TheAce0296/minecraft-server-cli",
  "bugs": "https://github.com/TheAce0296/minecraft-server-cli/issues",
  "bin": {
    "minecraft-server-cli": "./bin/run"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "node ./bin/dev",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts,.js --config .eslintrc.js",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "npm run lint",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "adm-zip": "^0.5.10",
    "inquirer": "^8.2.6",
    "semver": "^7.5.4",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.17.2",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "oclif": "^3.17.2",
    "prettier": "^3.0.1",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "minecraft-server-cli",
    "dirname": "minecraft-server-cli",
    "default": ".",
    "commands": "./dist/main",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  }
}
